{
  "name": "react-native-harmony-tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "cd ../react-native-harmony && npm pack && cd ../tester",
    "postinstall": "npm uninstall react-native-harmony && npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.72.0.tgz",
    "dev:rn": "npm i && npm run dev",
    "dev": "react-native bundle-harmony --prod",
    "android": "react-native run-android",
    "format": "prettier App.tsx ./tests -w",
    "start": "react-native start",
    "benchmark-fps": "node ./scripts/get-frame-times | node ./scripts/create-fps-stats",
    "test": "jest"
  },
  "dependencies": {
    "chai": "4.3.7",
    "d3-scale-chromatic": "^3.0.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.72.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.72.6",
    "@rnoh/testerino": "^0.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/chai": "^4.3.4",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.1",
    "@types/metro-config": "^0.76.2",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "csv-parser": "^3.0.0",
    "eslint": "^8.19.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.2.1",
    "metro": "^0.76.3",
    "metro-config": "^0.76.3",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "2.4.1",
    "react-test-renderer": "18.2.0",
    "simple-statistics": "^7.8.3",
    "typescript": "4.8.4",
    "yargs": "^17.7.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
