{
  "name": "react-native-harmony-tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "npm run pack_react_native_harmony && npm run pack_react_native_harmony_sample_package && npm run install:react_native_harmony && npm run install:react_native_harmony_sample_package && npm install && npm run install:hooks",
    "install:hooks": "cd .. && husky install tester/.husky && cd tester && if ! grep -q \"npm run verify\" .husky/pre-push; then npx husky add .husky/pre-push \"cd tester && npm run verify && cd ..\"; fi",
    "postinstall": "node ./scripts/create-build-profile && npm run reinstall:react_native_harmony && npm run reinstall:react_native_harmony_sample_package && npm run recreate_hvigor_plugin",
    "recreate_hvigor_plugin": "cd ../react-native-harmony-hvigor-plugin && npm i && npm run deploy && cd ../tester",
    "reinstall:react_native_harmony": "npm uninstall react-native-harmony && npm run pack_react_native_harmony && npm run install:react_native_harmony",
    "reinstall:react_native_harmony_sample_package": "npm uninstall react-native-harmony-sample-package && npm run pack_react_native_harmony_sample_package && npm run install:react_native_harmony_sample_package",
    "install:react_native_harmony": "npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.75.1.tgz",
    "install:react_native_harmony_sample_package": "npm i ../react-native-harmony-sample-package/react-native-harmony-sample-package-1.0.0.tgz",
    "pack_react_native_harmony": "cd ../react-native-harmony && npm i && npm pack && cd ../tester",
    "pack_react_native_harmony_sample_package": "cd ../react-native-harmony-sample-package && npm i && npm run codegen-lib && npm pack && cd ../tester",
    "install:local-cli": "cd ../react-native-harmony-cli && npm pack && cd ../tester && npm i ../react-native-harmony-cli/rnoh-react-native-harmony-cli-0.75.0.tgz",
    "dev:rn": "npm i && npm run dev",
    "dev": "react-native bundle-harmony --dev --minify=false",
    "prod": "react-native bundle-harmony --dev=false --minify=true --bundle-output /tmp/bundle.harmony.js && ./node_modules/react-native/sdks/hermesc/osx-bin/hermesc --emit-binary /tmp/bundle.harmony.js -out ./harmony/entry/src/main/resources/rawfile/hermes_bundle.hbc",
    "dev:hbc": "react-native bundle-harmony --dev --bundle-output /tmp/bundle.harmony.js && hermesc --emit-binary /tmp/bundle.harmony.js -out ./harmony/entry/src/main/resources/rawfile/hermes_bundle.hbc",
    "dev:multi_surface": "react-native codegen-harmony --rnoh-module-path ./harmony/react_native_openharmony --cpp-output-path ./harmony/multi_surface/src/main/cpp/generated  && react-native bundle-harmony --dev --bundle-output './harmony/multi_surface/src/main/resources/rawfile/bundle.harmony.js' --assets-dest './harmony/multi_surface/src/main/resources/rawfile/assets'",
    "dev:performance_measurement": "react-native codegen-harmony --rnoh-module-path ./harmony/react_native_openharmony --cpp-output-path ./harmony/performance_measurement/src/main/cpp/generated  && react-native bundle-harmony --dev --bundle-output './harmony/performance_measurement/src/main/resources/rawfile/bundle.harmony.js' --assets-dest './harmony/performance_measurement/src/main/resources/rawfile/assets'",
    "android": "react-native run-android",
    "format": "npm run format:js && npm run format:cpp",
    "format:js": "prettier App.tsx ./benchmarks ./components ./examples ./jests ./scripts ./tests -w",
    "format:cpp": "zsh -c 'clang-format -i ./harmony/react_native_openharmony/src/main/cpp/*.h ./harmony/react_native_openharmony/src/main/cpp/*.cpp ./harmony/react_native_openharmony/src/main/cpp/RNOH/**/*.h ./harmony/react_native_openharmony/src/main/cpp/RNOH/**/*.cpp ./harmony/react_native_openharmony/src/main/cpp/RNOHCorePackage/**/*.h ./harmony/react_native_openharmony/src/main/cpp/RNOHCorePackage/**/*.cpp ./harmony/sample_package/src/main/cpp/*.h ./harmony/sample_package/src/main/cpp/*.cpp ./harmony/entry/**/*.h ./harmony/entry/**/*.cpp --style=file:../.clang-format'",
    "format:cpp:check": "npm run --silent find_changed_files:cpp | sed 's#^#../#' | xargs clang-format --dry-run --Werror --style=file:../.clang-format",
    "typecheck": "tsc --noEmit",
    "lint:js": "eslint App.tsx ./benchmarks ./components ./examples ./jests ./scripts ./tests --max-warnings 0",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:cpp": "node ./scripts/lint-cpp-fast.js",
    "verify": "npm run typecheck && npm run test && npm run lint:js && npm run format:cpp:check && npm run check:deprecations && echo '\\nVerification Succeeded'",
    "check:deprecations": "ts-node ./scripts/check-deprecations.ts",
    "start": "react-native start",
    "benchmark-fps": "node ./scripts/get-frame-times | node ./scripts/create-fps-stats",
    "find_changed_files": "{ git diff --name-only $(git rev-list --exclude $(git branch --show-current) --branches -1) HEAD; git diff --name-only; git diff --name-only --cached; } | sort | uniq",
    "find_changed_files:cpp": "npm run find_changed_files | grep -E '\\.(h|cpp)$' | grep -v '/generated/' | grep -v '/third-party/' | grep -v '/patches/'",
    "test": "jest ./jests && cd ../react-native-harmony && npm test && cd ../tester",
    "hiprofiler": "./scripts/run-hiprofiler-cmd.sh",
    "measure-performance": "./scripts/performance_measurement/measure-performance.sh"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "chai": "4.3.7",
    "d3-scale-chromatic": "^3.0.0",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.75.1.tgz",
    "react-native-harmony-sample-package": "file:../react-native-harmony-sample-package/react-native-harmony-sample-package-1.0.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-oh-tpl/react-native-fs": "https://github.com/react-native-oh-library/react-native-fs/releases/download/2.20.0-0.1.9/react-native-oh-tpl-react-native-fs-2.20.0-0.1.9.tgz",
    "@react-native-oh-tpl/react-native-safe-area-context": "https://github.com/react-native-oh-library/react-native-safe-area-context/releases/download/4.7.4-0.1.4/react-native-oh-tpl-react-native-safe-area-context-4.7.4-0.1.4.tgz",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.19",
    "@rnoh/testerino": "^0.0.9",
    "@types/chai": "^4.3.4",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "csv-parser": "^3.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "node-html-parser": "^6.1.13",
    "prettier": "3.2.4",
    "react-test-renderer": "18.3.1",
    "simple-statistics": "^7.8.3",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4",
    "yargs": "^17.7.2"
  }
}
