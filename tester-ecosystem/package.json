{
  "name": "react-native-harmony-tester-ecosystem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "npm run _pack:react-native-harmony && npm run _install:react-native-harmony && npm run _install:cli && npm install",
    "build": "node ./scripts/build.js",
    "start": "react-native start",
    "harmony": "react-native run-harmony",
    "postinstall": "node ./scripts/create-build-profile && npm run _reinstall:react-native-harmony && npm run _reinstall:cli",
    "_reinstall:react-native-harmony": "npm uninstall react-native-harmony && npm run _pack:react-native-harmony && npm run _install:react-native-harmony",
    "_reinstall:cli": "npm uninstall react-native-harmony-cli && npm run _pack:cli && npm run _install:cli",
    "_install:react-native-harmony": "npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.77.0.tgz",
    "_install:cli": "npm i ../react-native-harmony-cli/rnoh-react-native-harmony-cli-0.77.0.tgz",
    "_pack:react-native-harmony": "cd ../react-native-harmony && npm i && npm run build:har && npm pack && cd ../tester-ecosystem",
    "_pack:cli": "cd ../react-native-harmony-cli && npm pack && cd ../tester-ecosystem"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@react-native-oh-tpl/async-storage": "^1.21.0-0.2.2",
    "@react-native-oh/react-native-core": "npm:@rnoh/react-native-core@0.77.0-rn.0.77.0",
    "@rnoh/react-native-harmony-cli": "file:../react-native-harmony-cli/rnoh-react-native-harmony-cli-0.77.0.tgz",
    "@rnoh/react-native-harmony-gesture-handler": "^2.20.1",
    "chai": "4.3.7",
    "react": "18.3.1",
    "react-native": "0.77.0",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.77.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@rnoh/testerino": "^0.0.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.6",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
