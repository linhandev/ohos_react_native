{
  "name": "react-native-harmony-tester-ecosystem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "npm run pack_react_native_harmony && npm run install:react_native_harmony && npm install",
    "postinstall": "node ./scripts/create-build-profile",
    "start": "react-native start",
    "build": "node ./scripts/build.js",
    "reinstall:react_native_harmony": "npm uninstall react-native-harmony && npm run pack_react_native_harmony && npm run install:react_native_harmony",
    "install:react_native_harmony": "npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.75.2.tgz",
    "pack_react_native_harmony": "cd ../react-native-harmony && npm i && npm run build:har && npm pack && cd ../tester-ecosystem",
    "harmony": "react-native run-harmony"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@rnoh/react-native-harmony-gesture-handler": "^2.20.1",
    "chai": "4.3.7",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.75.2.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "^0.75.4",
    "@rnoh/testerino": "^0.0.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.6",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
